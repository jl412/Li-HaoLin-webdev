            <h4>Javascript</h4>
<div class="highlight"><pre><code class="language-javascript" data-lang="javascript"><span class="nx">angular</span><span class="p">.</span><span class="nx">module</span><span class="p">(</span><span class="s2">"angular-table-example"</span><span class="p">).</span><span class="nx">controller</span><span class="p">(</span><span class="s2">"interactiveExampleCtrl"</span><span class="p">,</span> <span class="p">[</span><span class="s2">"$scope"</span><span class="p">,</span> <span class="s2">"$filter"</span><span class="p">,</span> <span class="kd">function</span><span class="p">(</span><span class="nx">$scope</span><span class="p">,</span> <span class="nx">$filter</span><span class="p">)</span> <span class="p">{</span>
  <span class="nx">$scope</span><span class="p">.</span><span class="nx">originalList</span> <span class="o">=</span> <span class="nx">$scope</span><span class="p">.</span><span class="nx">$parent</span><span class="p">.</span><span class="nx">personList</span><span class="p">;</span>

  <span class="nx">$scope</span><span class="p">.</span><span class="nx">filteredList</span> <span class="o">=</span> <span class="nx">$scope</span><span class="p">.</span><span class="nx">originalList</span><span class="p">;</span>

  <span class="nx">$scope</span><span class="p">.</span><span class="nx">config</span> <span class="o">=</span> <span class="p">{</span>
    <span class="nx">itemsPerPage</span><span class="o">:</span> <span class="mi">5</span><span class="p">,</span>
    <span class="nx">maxPages</span><span class="o">:</span> <span class="mi">5</span><span class="p">,</span>
    <span class="nx">fillLastPage</span><span class="o">:</span> <span class="s2">"yes"</span>
  <span class="p">};</span>

  <span class="nx">$scope</span><span class="p">.</span><span class="nx">add</span> <span class="o">=</span> <span class="kd">function</span><span class="p">()</span> <span class="p">{</span>
    <span class="nx">$scope</span><span class="p">.</span><span class="nx">originalList</span><span class="p">.</span><span class="nx">push</span><span class="p">({</span><span class="nx">name</span><span class="o">:</span> <span class="nx">$scope</span><span class="p">.</span><span class="nx">nameToAdd</span><span class="p">});</span>
    <span class="nx">$scope</span><span class="p">.</span><span class="nx">updateFilteredList</span><span class="p">();</span>
  <span class="p">}</span>

  <span class="nx">$scope</span><span class="p">.</span><span class="nx">updateFilteredList</span> <span class="o">=</span> <span class="kd">function</span><span class="p">()</span> <span class="p">{</span>
    <span class="nx">$scope</span><span class="p">.</span><span class="nx">filteredList</span> <span class="o">=</span> <span class="nx">$filter</span><span class="p">(</span><span class="s2">"filter"</span><span class="p">)(</span><span class="nx">$scope</span><span class="p">.</span><span class="nx">originalList</span><span class="p">,</span> <span class="nx">$scope</span><span class="p">.</span><span class="nx">query</span><span class="p">);</span>
  <span class="p">};</span>
<span class="p">}])</span></code></pre></div>
          </div>
        </div>
      </angular-tabs>
    </div>

    <div tab-title="Rendering Cells" ng-show="isActive(&#39;Rendering Cells&#39;)" class="ng-hide">
<div ng-controller="renderingCellsCtrl" class="ng-scope">
  <h3>Using <code>at-implicit</code> And <code>at-attribute</code></h3>
    <div class="row">
      <div class="col-md-3">
        <table class="table table-striped table-bordered ng-scope" at-table="" at-list="list">
  <thead><tr><th width="">Name</th></tr></thead>
  <tbody>
    <!-- ngRepeat: item in list | orderBy:predicate:descending --><tr ng-repeat="item in list | orderBy:predicate:descending" class="ng-scope"> <td at-implicit="" at-attribute="name"><span ng-bind="item.name" class="ng-binding">samu</span></td> </tr><!-- end ngRepeat: item in list | orderBy:predicate:descending -->
  </tbody>
</table>
      </div>
      <div class="col-md-9">
<div class="highlight"><pre><code class="language-html" data-lang="html"><span class="nt">&lt;table</span> <span class="na">class=</span><span class="s">"table table-striped table-bordered"</span>
  <span class="na">at-table</span> <span class="na">at-list=</span><span class="s">"list"</span><span class="nt">&gt;</span>
  <span class="nt">&lt;thead&gt;&lt;/thead&gt;</span>
  <span class="nt">&lt;tbody&gt;</span>
    <span class="nt">&lt;tr&gt;</span> <span class="nt">&lt;td</span> <span class="na">at-implicit</span> <span class="na">at-attribute=</span><span class="s">"name"</span><span class="nt">&gt;&lt;/td&gt;</span> <span class="nt">&lt;/tr&gt;</span>
  <span class="nt">&lt;/tbody&gt;</span>
<span class="nt">&lt;/table&gt;</span></code></pre></div>
      </div>
    </div>

  <h3>Using  <code>item</code> And Custom Markup</h3>
    <div class="row">
      <div class="col-md-3">
        
<table class="table table-striped table-bordered ng-scope" at-table="" at-list="list">
  <thead><tr><th width="">Name</th><th width="">Length</th></tr></thead>
  <tbody>
    <!-- ngRepeat: item in list | orderBy:predicate:descending --><tr ng-repeat="item in list | orderBy:predicate:descending" class="ng-scope">
      <td at-title="Name" class="ng-binding">
        Hello samu
      </td>
      <td at-title="Length">
        <code class="ng-binding">4</code>
      </td>
    </tr><!-- end ngRepeat: item in list | orderBy:predicate:descending -->
  </tbody>
</table>

      </div>
      <div class="col-md-9" ng-non-bindable="">
<div class="highlight"><pre><code class="language-html" data-lang="html"><span class="nt">&lt;table</span> <span class="na">class=</span><span class="s">"table table-striped table-bordered"</span>
  <span class="na">at-table</span> <span class="na">at-list=</span><span class="s">"list"</span><span class="nt">&gt;</span>
  <span class="nt">&lt;thead&gt;&lt;/thead&gt;</span>
  <span class="nt">&lt;tbody&gt;</span>
    <span class="nt">&lt;tr&gt;</span>
      <span class="nt">&lt;td</span> <span class="na">at-title=</span><span class="s">"Name"</span><span class="nt">&gt;</span>
        Hello {{item.name}}
      <span class="nt">&lt;/td&gt;</span>
      <span class="nt">&lt;td</span> <span class="na">at-title=</span><span class="s">"Length"</span><span class="nt">&gt;</span>
        <span class="nt">&lt;code&gt;</span>{{item.name.length}}<span class="nt">&lt;/code&gt;</span>
      <span class="nt">&lt;/td&gt;</span>
    <span class="nt">&lt;/tr&gt;</span>
  <span class="nt">&lt;/tbody&gt;</span>
<span class="nt">&lt;/table&gt;</span></code></pre></div>
      </div>
    </div>
